set style data linespoints
set logscale x
set yrange [0:]
set xrange [10:]
set xlabel "Value size [Bytes]"
set ylabel "K Operations per second"
set key right
set output "remove-iops.eps"
set term postscript eps "Times-Roman" 18
plot 'remove-1-iops.dat' title '1 thread', \
     'remove-2-iops.dat' title '2 threads', \
     'remove-3-iops.dat' title '3 threads', \
     'remove-4-iops.dat' title '4 threads', \
     'remove-5-iops.dat' title '5 threads', \
     'remove-6-iops.dat' title '6 threads', \
     'remove-7-iops.dat' title '7 threads', \
     'remove-8-iops.dat' title '8 threads'
