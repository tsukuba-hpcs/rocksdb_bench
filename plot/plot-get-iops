set style data linespoints
set logscale x
set yrange [0:]
set xrange [10:]
set xlabel "Value size [Bytes]"
set ylabel "K Operations per second"
set key right
set output "get-iops.eps"
set term postscript eps "Times-Roman" 18 color
plot 'get-1-iops.dat' title '1 thread', \
     'get-2-iops.dat' title '2 threads', \
     'get-3-iops.dat' title '3 threads', \
     'get-4-iops.dat' title '4 threads', \
     'get-5-iops.dat' title '5 threads', \
     'get-6-iops.dat' title '6 threads', \
     'get-7-iops.dat' title '7 threads', \
     'get-8-iops.dat' title '8 threads'
